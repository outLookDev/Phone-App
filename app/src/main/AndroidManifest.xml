<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.azmovhudstc.murakkabview">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher_foreground"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_launcher_background"
        android:supportsRtl="true"
        android:theme="@style/Theme.MurakkabView">
        <activity
            android:name=".activitys.phone.PhoneAboutActivity"
            android:exported="false" />
        <activity
            android:name=".activitys.phone.PhoneListChooseActivity"
            android:exported="false" />
        <activity
            android:name=".activitys.phone.AddPhoneActivity"
            android:exported="false" />
        <activity
            android:name=".activitys.phone.HomeActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activitys.MainActivity"
            android:exported="true" />
        <activity android:name=".activitys.phone.ChooseTypePhones" />
        <activity android:name=".activitys.phone.PhoneItemActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>